{
    "name": "phpdish/plugin-skeleton",
    "type": "phpdish-plugin",
    "description": "Acme example plugin for PHPDish application.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "phpdish/phpdish": "*@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6|^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Acme\\PHPDishExamplePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\PHPDishExamplePlugin\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
